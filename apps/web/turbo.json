{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"],
      "env": [],
      "passThroughEnv": ["VERCEL_*", "NEXT_*", "TURBO_*", "HOME", "CI", "VERCEL", "FLUENT_*", "USE_OUTPUT_FOR_EDGE_FUNCTIONS", "PATH", "NODE_*", "NODE_ENV", "TZ", "NOW_*", "PWD", "SHLVL", "LANG", "FAKEROOTKEY", "ENABLE_VC_BUILD", "ECS_*", "AWS_*", "DD_*"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
